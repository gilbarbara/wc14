<% time = WC.formatDate(startTime) %>
<div class="match">
	<div class="match-game row <%= status !== 'Pre-game' ? 'final' : '' %>" data-id="<%= id %>">
		<div class="col-xs-5 left" style="background-image:url('<%= WC.getFlag(homeTeamId) %>');"><%= WC.getTeam(homeTeamId).name %></div>
		<div class="col-xs-2 center">
			<% if(status !== 'Pre-game') { %>
			<div class="goals"><%= homeScore %> : <%= awayScore %></div>
			<% } else { %>
			<%= WC.formatDate(startTime) %>
			<% } %>
		</div>
		<div class="col-xs-5 right" style="background-image:url('<%= WC.getFlag(awayTeamId) %>');"><%= WC.getTeam(awayTeamId).name %></div>
		<div class="col-xs-2 time"><%= WC.formatDate(startTime) %></div>
		<div class="col-xs-10 venue"><%= WC.data.venues[venue] %></div>
	</div>
</div>